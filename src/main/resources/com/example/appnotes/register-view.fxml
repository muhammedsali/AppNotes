<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.appnotes.RegisterController" stylesheets="@styles.css">
    <TextField fx:id="usernameField" promptText="Username" minWidth="200" />
    <PasswordField fx:id="passwordField" promptText="Password" minWidth="200" />
    <Button text="Register" onAction="#register" minWidth="100" />
    <Button text="Back to Login" onAction="#showLogin" minWidth="100" />
</VBox>
